{
  "name": "@augur-ai/analytics-react",
  "version": "0.1.1",
  "description": "React hooks for Augur Analytics SDK - Easy integration with batching and device detection",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "augur",
    "analytics",
    "react",
    "hooks",
    "session",
    "tracking",
    "telemetry",
    "device-detection",
    "rum",
    "real-user-monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/augur-ai/augur-analytics.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/augur-ai/augur-analytics/issues"
  },
  "homepage": "https://github.com/augur-ai/augur-analytics#readme",
  "author": "Augur Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@augur-ai/analytics-core": "^0.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
